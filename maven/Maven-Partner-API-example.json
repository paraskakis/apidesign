{
  "openapi": "3.1.0",
  "info": {
    "title": "Maven Partner API",
    "version": "1.0.1",
    "description": "API for Maven Instructors to manage courses, cohorts, and student data.",
    "contact": {
      "name": "Maven API Support",
      "url": "https://maven.com/support",
      "email": "api-support@maven.com"
    },
    "license": {
      "name": "Maven API License",
      "url": "https://maven.com/legal/api-license"
    }
  },
  "servers": [
    {
      "url": "https://api.maven.com",
      "description": "Production Server"
    }
  ],
  "tags": [
    {
      "name": "Instructors",
      "description": "Operations related to Instructors and their courses"
    },
    {
      "name": "Courses",
      "description": "Operations related to Course management and intelligence"
    },
    {
      "name": "Cohorts",
      "description": "Operations related to Cohort management and enrollment"
    },
    {
      "name": "Students",
      "description": "Operations related to Student insights and history"
    }
  ],
  "paths": {
    "/instructors/{instructorId}/courses": {
      "get": {
        "summary": "View Instructor Courses",
        "description": "Retrieve a list of courses for a specific instructor, filtering by status.",
        "operationId": "getInstructorCourses",
        "tags": [
          "Instructors"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/InstructorIdPath"
          },
          {
            "$ref": "#/components/parameters/StatusQuery"
          },
          {
            "$ref": "#/components/parameters/LimitQuery"
          },
          {
            "$ref": "#/components/parameters/OffsetQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "A list of courses",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CourseList"
                },
                "example": [
                  {
                    "name": "API Fundamentals for Product Managers",
                    "id": "api-pm-fundamentals",
                    "averageRating": 4.5,
                    "price": 999.0,
                    "landingPageUrl": "https://maven.com/emmanuel/api-pm-fundamentals",
                    "studentWaitlist": []
                  }
                ]
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      }
    },
    "/courses/{courseId}/cohorts/{cohortId}/earnings": {
      "get": {
        "summary": "View Cohort Earnings",
        "description": "Retrieve financial performance for a specific cohort.",
        "operationId": "getCohortEarnings",
        "tags": [
          "Courses"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/CourseIdPath"
          },
          {
            "$ref": "#/components/parameters/CohortIdPath"
          }
        ],
        "responses": {
          "200": {
            "description": "Earnings data for the cohort",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Earnings"
                },
                "example": {
                  "totalNetEarnings": 15000.0,
                  "currency": "USD"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      }
    },
    "/courses/{courseId}/earnings": {
      "get": {
        "summary": "View Course Earnings",
        "description": "Retrieve aggregated earnings for a whole course.",
        "operationId": "getCourseEarnings",
        "tags": [
          "Courses"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/CourseIdPath"
          }
        ],
        "responses": {
          "200": {
            "description": "Aggregated earnings data for the course",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Earnings"
                },
                "example": {
                  "totalNetEarnings": 45000.0,
                  "currency": "USD"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      }
    },
    "/courses/{courseId}/ratings/summary": {
      "get": {
        "summary": "Analyze Course Ratings",
        "description": "Get average rating for a course, optionally filtered by cohort.",
        "operationId": "getCourseRatingsSummary",
        "tags": [
          "Courses"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/CourseIdPath"
          },
          {
            "$ref": "#/components/parameters/CohortIdQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Rating summary",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RatingSummary"
                },
                "example": {
                  "averageRating": 4.7,
                  "totalRatings": 150
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      }
    },
    "/cohorts/{cohortId}/students": {
      "get": {
        "summary": "View Enrolled Students",
        "description": "List all students currently enrolled in a specific cohort.",
        "operationId": "getEnrolledStudents",
        "tags": [
          "Cohorts"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/CohortIdPath"
          },
          {
            "$ref": "#/components/parameters/LimitQuery"
          },
          {
            "$ref": "#/components/parameters/OffsetQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "List of enrolled students",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StudentList"
                },
                "example": [
                  {
                    "name": "Alice Smith",
                    "email": "alice@example.com"
                  }
                ]
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      },
      "post": {
        "summary": "Enroll Student",
        "description": "Enroll a student in a specific cohort. Creates the student record 'Just-In-Time' if they are new.",
        "operationId": "enrollStudent",
        "tags": [
          "Cohorts"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/CohortIdPath"
          }
        ],
        "requestBody": {
          "description": "Student information for enrollment",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Student"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Student successfully enrolled",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EnrollmentConfirmation"
                },
                "example": {
                  "cohortId": 6,
                  "studentEmail": "alice@example.com",
                  "status": "enrolled"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      }
    },
    "/cohorts/{cohortId}/students/{studentEmail}": {
      "delete": {
        "summary": "Drop Student",
        "description": "Remove a student from the cohort's enrolled list (drops the student).",
        "operationId": "dropStudent",
        "tags": [
          "Cohorts"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/CohortIdPath"
          },
          {
            "$ref": "#/components/parameters/StudentEmailPath"
          }
        ],
        "responses": {
          "204": {
            "description": "Student successfully dropped (no content returned)",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      }
    },
    "/courses/{courseId}/waitlist": {
      "post": {
        "summary": "Manage Waitlist (Upsert)",
        "description": "Add a student to a course waitlist. Creates the student record if they are new.",
        "operationId": "addToWaitlist",
        "tags": [
          "Courses"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/CourseIdPath"
          }
        ],
        "requestBody": {
          "description": "Student information to add to waitlist",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Student"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Student added to waitlist successfully",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WaitlistConfirmation"
                },
                "example": {
                  "courseId": "api-pm-fundamentals",
                  "studentEmail": "alice@example.com",
                  "status": "waitlisted"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      }
    },
    "/students/{studentEmail}/enrollments": {
      "get": {
        "summary": "Search Student Enrollment History",
        "description": "View a specific student's full enrollment history.",
        "operationId": "getStudentEnrollmentHistory",
        "tags": [
          "Students"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/StudentEmailPath"
          },
          {
            "$ref": "#/components/parameters/LimitQuery"
          },
          {
            "$ref": "#/components/parameters/OffsetQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Student's enrollment history",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EnrollmentHistoryList"
                },
                "example": [
                  {
                    "courseName": "API Fundamentals",
                    "cohortName": "Cohort 6",
                    "enrollmentDate": "2025-12-01T10:00:00Z",
                    "status": "enrolled"
                  }
                ]
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      }
    },
    "/students/{studentEmail}/earnings": {
      "get": {
        "summary": "Search Student Earnings",
        "description": "See total net earnings generated by a specific student.",
        "operationId": "getStudentEarnings",
        "tags": [
          "Students"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/StudentEmailPath"
          }
        ],
        "responses": {
          "200": {
            "description": "Total earnings from student",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Earnings"
                },
                "example": {
                  "totalNetEarnings": 2500.0,
                  "currency": "USD"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      }
    },
    "/courses/{courseId}/ratings": {
      "get": {
        "summary": "Filter Student Ratings",
        "description": "Search through student ratings by score or email.",
        "operationId": "filterRatings",
        "tags": [
          "Courses"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/CourseIdPath"
          },
          {
            "$ref": "#/components/parameters/MinimumRatingQuery"
          },
          {
            "$ref": "#/components/parameters/MaximumRatingQuery"
          },
          {
            "$ref": "#/components/parameters/StudentEmailQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "List of filtered ratings",
            "headers": {
              "X-RateLimit-Limit": {
                "$ref": "#/components/headers/X-RateLimit-Limit"
              },
              "X-RateLimit-Remaining": {
                "$ref": "#/components/headers/X-RateLimit-Remaining"
              },
              "X-RateLimit-Reset": {
                "$ref": "#/components/headers/X-RateLimit-Reset"
              }
            },
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RatingList"
                },
                "example": [
                  {
                    "studentEmail": "alice@example.com",
                    "rating": 5,
                    "comment": "Great course!",
                    "date": "2026-01-20T10:00:00Z"
                  }
                ]
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "429": {
            "$ref": "#/components/responses/TooManyRequests"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Course": {
        "description": "A cohort-based course offered by a school.",
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the course"
          },
          "id": {
            "type": "string",
            "description": "Human-readable URL-safe identifier (slug).",
            "pattern": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
          },
          "averageRating": {
            "type": "number",
            "description": "Average rating of the course (1 to 5)",
            "minimum": 1,
            "maximum": 5
          },
          "price": {
            "type": "number",
            "description": "Price of the course in USD",
            "format": "currency"
          },
          "landingPageUrl": {
            "type": "string",
            "description": "URL of the course landing page",
            "format": "uri"
          },
          "studentWaitlist": {
            "type": "array",
            "description": "List of students on the waitlist",
            "items": {
              "$ref": "#/components/schemas/Student"
            }
          }
        },
        "required": [
          "name",
          "id",
          "price",
          "landingPageUrl"
        ],
        "example": {
          "name": "API Fundamentals for Product Managers",
          "id": "api-pm-fundamentals",
          "averageRating": 4.5,
          "price": 999.0,
          "landingPageUrl": "https://maven.com/emmanuel/api-pm-fundamentals",
          "studentWaitlist": []
        }
      },
      "CourseList": {
        "description": "A list of courses.",
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Course"
        }
      },
      "Student": {
        "description": "A student user on the platform.",
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Full name of the student"
          },
          "email": {
            "type": "string",
            "description": "Email address of the student",
            "format": "email"
          }
        },
        "required": [
          "name",
          "email"
        ],
        "example": {
          "name": "Alice Smith",
          "email": "alice@example.com"
        }
      },
      "StudentList": {
        "description": "A list of students.",
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Student"
        }
      },
      "Earnings": {
        "description": "Financial earnings information.",
        "type": "object",
        "properties": {
          "totalNetEarnings": {
            "type": "number",
            "description": "Total net earnings amount"
          },
          "currency": {
            "type": "string",
            "description": "Currency code",
            "default": "USD"
          }
        },
        "required": [
          "totalNetEarnings"
        ],
        "example": {
          "totalNetEarnings": 15000.0,
          "currency": "USD"
        }
      },
      "RatingSummary": {
        "description": "Summary of ratings for a course or cohort.",
        "type": "object",
        "properties": {
          "averageRating": {
            "type": "number",
            "description": "Average calculated rating"
          },
          "totalRatings": {
            "type": "integer",
            "description": "Total count of ratings received"
          }
        },
        "required": [
          "averageRating",
          "totalRatings"
        ],
        "example": {
          "averageRating": 4.7,
          "totalRatings": 150
        }
      },
      "WaitlistConfirmation": {
        "description": "Confirmation of waitlist addition.",
        "type": "object",
        "properties": {
          "courseId": {
            "type": "string",
            "description": "ID of the course"
          },
          "studentEmail": {
            "type": "string",
            "description": "Email of the student",
            "format": "email"
          },
          "status": {
            "type": "string",
            "description": "Current status on waitlist",
            "enum": [
              "waitlisted"
            ]
          }
        },
        "required": [
          "courseId",
          "studentEmail",
          "status"
        ],
        "example": {
          "courseId": "api-pm-fundamentals",
          "studentEmail": "alice@example.com",
          "status": "waitlisted"
        }
      },
      "EnrollmentConfirmation": {
        "description": "Confirmation of enrollment.",
        "type": "object",
        "properties": {
          "cohortId": {
            "type": "integer",
            "description": "ID of the cohort"
          },
          "studentEmail": {
            "type": "string",
            "description": "Email of the student",
            "format": "email"
          },
          "status": {
            "type": "string",
            "description": "New enrollment status",
            "enum": [
              "enrolled"
            ]
          }
        },
        "required": [
          "cohortId",
          "studentEmail",
          "status"
        ],
        "example": {
          "cohortId": 6,
          "studentEmail": "alice@example.com",
          "status": "enrolled"
        }
      },
      "EnrollmentHistoryItem": {
        "description": "A single record in a student's enrollment history.",
        "type": "object",
        "properties": {
          "courseName": {
            "type": "string",
            "description": "Name of the course"
          },
          "cohortName": {
            "type": "string",
            "description": "Name of the cohort"
          },
          "enrollmentDate": {
            "type": "string",
            "description": "Date of enrollment",
            "format": "date-time"
          },
          "status": {
            "type": "string",
            "description": "Enrollment status",
            "enum": [
              "enrolled",
              "dropped",
              "waitlisted"
            ]
          }
        },
        "required": [
          "courseName",
          "cohortName",
          "enrollmentDate",
          "status"
        ],
        "example": {
          "courseName": "API Fundamentals",
          "cohortName": "Cohort 6",
          "enrollmentDate": "2025-12-01T10:00:00Z",
          "status": "enrolled"
        }
      },
      "EnrollmentHistoryList": {
        "description": "List of enrollment history items.",
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/EnrollmentHistoryItem"
        }
      },
      "RatingItem": {
        "description": "A single rating record.",
        "type": "object",
        "properties": {
          "studentEmail": {
            "type": "string",
            "description": "Email of the reviewer",
            "format": "email"
          },
          "rating": {
            "type": "integer",
            "description": "Rating score",
            "minimum": 1,
            "maximum": 5
          },
          "comment": {
            "type": ["string", "null"],
            "description": "Optional text comment"
